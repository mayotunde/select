L.geoJson(cities, {
pointToLayer: function(feature, latlng){
marker = L.marker(latlng);
marker.bindPopup('<strong style="color:red">' + feature.properties.address + '</strong>')
return marker;
}}).addTo(done);

done.on('mouseover', function(e) {
e.layer.closePopup();
var feature = e.layer.feature;
var content = '<div><strong>' + feature.properties.address + '</strong>' + '<p><em>' + feature.properties.country + '</em></p></div>';
info.innerHTML = content; 
});


</script>

<style>
.info{ position:absolute;top:20px;right: 490px;}
.info div{padding: 20px; background: #f45; border: 1px solid black;}
</style>
</body>
</html>
