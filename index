L.geoJson(facilities2, {
pointToLayer: function(feature, latlng) {
marker = new L.marker(latlng)
marker.bindPopup(feature.properties.FACILITY)
return marker;
}}).addTo(facilitiesCluster);

var Bounds = L.layerGroup();

var cluster1 = L.latLngBounds([[6.4467, 3.3064], [6.4208, 3.3503]]);
L.rectangle(cluster1, {color: 'black', weight:2, fillColor: 'transparent'}).addTo(Bounds);

var cluster2 = L.latLngBounds([[6.4653, 3.3507], [6.4035, 3.4081]]);
L.rectangle(cluster2, {color: 'black', weight:2, fillColor: 'transparent'}).addTo(Bounds);
  

var map = L.map('map', {center: [6.4632778, 3.3657778], zoom: 12, layers: [base, facilities, facilitiesCluster, Bounds]});

var baselayer = {'Streets': base, "Satellite": satellite};

var overlay = {'Bounds': Bounds, 'Clusters': facilitiesCluster, 'Facilities': facilities};

L.control.layers(baselayer, overlay, {collapsed: false}).addTo(map);
